{
    "name": "david-weber/dominion",
    "description": "A simple, clean admin package for Laravel 4 with no other dependencies.",
    "keywords": ["laravel", "admin", "auth", "acl", "modules"],
    "license": "Apache 2",
    "authors": [
        {
            "name": "David Weber",
            "email": "dpweberza@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "4.1.x"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "1.*",
        "phpunit/phpunit": "3.7.*",
        "codeception/codeception": "1.6.1.1"
    },
    "autoload": {
        "psr-0": {
            "DavidWeber\\Dominion": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "minimum-stability": "dev"
}